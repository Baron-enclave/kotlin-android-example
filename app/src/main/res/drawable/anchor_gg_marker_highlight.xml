<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="25dp"
        android:height="25dp"
        android:gravity="center_horizontal"
        android:top="12dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#FFF" />
                <stroke
                    android:width="1dp"
                    android:color="@color/darker_gray" />
            </shape>
        </rotate>
    </item>
    <item
        android:width="65dp"
        android:height="35dp"
        android:bottom="16dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFF" />
            <corners android:radius="50dp" />
            <stroke
                android:width="1dp"
                android:color="@color/darker_gray" />
        </shape>
    </item>
    <item
        android:width="24dp"
        android:height="24dp"
        android:gravity="center_horizontal"
        android:top="12dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#FFF" />

            </shape>
        </rotate>
    </item>
</layer-list>
